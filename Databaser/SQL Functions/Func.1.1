DELIMITER //
DROP FUNCTION IF EXISTS fibonacci;
CREATE FUNCTION fibonacci(n INT)
RETURNS INT
BEGIN
		DECLARE i INT;
		DECLARE a INT;
		DECLARE b INT;
		DECLARE c INT;
		SET a = 0;  -- s√¶tter fibonacci til at starte fra 0
		SET b = 1;
		SET c = 0;
		SET i = 1;
		WHILE (i < n) DO
				SET c = a + b;
				SET a = b;
				SET b = c;
				SET i = i + 1;
		END WHILE;
		RETURN c;
END;//
DELIMITER ;

SELECT fibonacci(8);
